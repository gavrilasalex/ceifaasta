<mvc:View
	controllerName="org.ubb.hw.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true">
	<Shell>
		<App>
			<pages>
				<Page title="{i18n>homePageTitle}">
				<headerContent>
						<Button
							icon="sap-icon://hello-world"
							press=".onOpenDialog"/>
					</headerContent>
					<content>
						<mvc:XMLView viewName="org.ubb.hw.view.HW"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>