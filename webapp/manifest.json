{
    "_version": "1.12.0",
    "sap.app": {
      "id": "org.ubb.hw",
      "type": "application",
      "i18n": "i18n/i18n.properties",
      "title": "{{appTitle}}",
      "description": "{{appDescription}}",
      "applicationVersion": {
        "version": "1.0.0"
      },
      "dataSources": {
        "booksService": {
          "uri": "/sap/opu/odata/SAP/ZANDATEST_SRV/",
          "type": "OData",
          "settings": {
            "odataVersion": "2.0"
          }
        }
      }
    },
    "sap.ui": {
      "technology": "UI5",
      "deviceTypes": {
          "desktop": true,
          "tablet": true,
          "phone": true
      }
    },
    "sap.ui5": {
      "rootView": {
          "viewName": "org.ubb.hw.view.App",
          "type": "XML",
          "async": true,
          "id": "app"
      },
      "dependencies": {
        "minUI5Version": "1.60",
        "libs": {
          "sap.m": {}
        }
      },
      "models": {
        "i18n": {
          "type": "sap.ui.model.resource.ResourceModel",
          "settings": {
            "bundleName": "org.ubb.hw.i18n.i18n"
          }
        },
        "invoice": {
          "type": "sap.ui.model.json.JSONModel",
          "uri": "Invoices.json"
        },
        "": {
            "dataSource": "booksService",
            "settings": {
              "metadataUrlParams": {
                "sap-documentation": "heading"
              },
              "defaultBindingMode": "TwoWay"
            }
          }
      },
      "resources": {
        "css": [
          {
            "uri": "css/style.css"
          }
        ]
      },
      "routing": {
        "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "org.ubb.hw.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
        },
        "routes": [
        {
          "pattern": "",
          "name": "login",
          "target": "login"
        },
        {
          "pattern": "detail/{invoicePath}",
          "name": "detail",
          "target": "detail"
        },
        {
          "pattern": "overview",
          "name": "overview",
          "target": "overview"
        }
        ],
        "targets": {
        "login": {
          "viewId": "login",
          "viewName": "Login"
        },
        "detail": {
          "viewId": "detail",
          "viewName": "Detail"
        },
        "overview": {
          "viewId": "overview",
          "viewName": "Overview"
        }
        }
      }
    }
  }